Cliente
    nome                Text
    endereco            Text
    telefone            Int
    celular             Int
    email               Text
    senha               Text
    tratid              TratamentoId

Tratamento
    nome                Text
    nro_sessoes         Int
    valor               Double
    descricao           Text

Funcionario
    nome                Text
    email               Text
    senha               Text
    
Terapeuta
    nome                Text
    descricao           Text
    avatar              Text
    tratid              TratamentoId

Consulta
    cliente             ClienteId
    terapeuta           TerapeutaId
    dt_consulta         Day
    hr_consulta         Day

